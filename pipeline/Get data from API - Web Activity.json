				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "APIOut",
					"value": {
						"value": "@activity('GetUsers_FromAPI').output.data",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"APIOut": {
				"type": "Array"
			}
		},
		"annotations": []
	}
