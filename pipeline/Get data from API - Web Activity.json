{
	"name": "Get data from API - Web Activity",
	"properties": {
		"description": "- Web Activity\n- Set Variable",
		"activities": [
			{
				"name": "GetUsers_FromAPI",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
